<Addin 
	id="Mounter"
	namespace="Docky"
	version="1.0"
	isroot="false"
	defaultEnabled="true"
	name="Mounter"
>
 
 	<Dependencies>
		<Addin id="Items" version="1.0" />
	</Dependencies>
 
	<Runtime>
		<Import assembly="Mounter.dll"/>
	</Runtime>
	
	<Extension path="/Docky/ItemProvider">
		<ItemProvider type="Mounter.MountProvider" />
	</Extension>	

</Addin>
