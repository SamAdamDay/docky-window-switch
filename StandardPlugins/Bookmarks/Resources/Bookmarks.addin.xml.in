<Addin 
	id="Bookmarks"
	namespace="Docky"
	version="1.0"
	isroot="false"
	defaultEnabled="false"
	name="Bookmarks"
>
 
 	<Dependencies>
		<Addin id="Items" version="1.0" />
	</Dependencies>
 
	<Runtime>
		<Import assembly="Bookmarks.dll"/>
	</Runtime>
	
	<Extension path="/Docky/ItemProvider">
		<ItemProvider type="Bookmarks.BookmarksItemProvider" />
	</Extension>	

</Addin>
