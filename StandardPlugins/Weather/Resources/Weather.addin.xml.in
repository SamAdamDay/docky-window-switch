<Addin 
	id="Weather"
	namespace="Docky"
	version="1.0"
	isroot="false"
	defaultEnabled="false"
	name="Weather"
>
 
 	<Dependencies>
		<Addin id="Items" version="1.0" />
	</Dependencies>
 
	<Runtime>
		<Import assembly="Weather.dll"/>
	</Runtime>
	
	<Extension path="/Docky/ItemProvider">
		<ItemProvider type="WeatherDocklet.WeatherItemProvider" />
	</Extension>	

</Addin>
