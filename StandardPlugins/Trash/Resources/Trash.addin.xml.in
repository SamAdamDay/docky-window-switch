<Addin 
	id="Trash"
	namespace="Docky"
	version="1.0"
	isroot="false"
	defaultEnabled="false"
	name="Trash"
	description="Have a look at your trash!"
	author="Chris Szikszoy, Robert Dyer"
>
 
 	<Dependencies>
		<Addin id="Items" version="1.0" />
	</Dependencies>
 
	<Runtime>
		<Import assembly="Trash.dll"/>
	</Runtime>
	
	<Extension path="/Docky/ItemProvider">
		<ItemProvider type="Trash.TrashItemProvider" />	
	</Extension>	

</Addin>
