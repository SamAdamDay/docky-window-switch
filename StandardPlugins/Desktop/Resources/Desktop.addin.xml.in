<Addin 
	id="Desktop"
	namespace="Docky"
	version="1.0"
	isroot="false"
	defaultEnabled="false"
	name="Desktop"
	description="Show Desktop Toggler"
	author="Rico Tzschichholz"
>
 
 	<Dependencies>
		<Addin id="Items" version="1.0" />
	</Dependencies>
 
	<Runtime>
		<Import assembly="Desktop.dll"/>
	</Runtime>
	
	<Extension path="/Docky/ItemProvider">
		<ItemProvider type="Desktop.DesktopItemProvider" />
	</Extension>	

</Addin>
